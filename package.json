{
  "name": "daily-worklog-app",
  "productName": "SASV Workspace",
  "version": "3.1.0",
  "description": "Pilot Electron + Supabase workspace application",
  "main": "main.js",
  "scripts": {
    "dev": "electron .",
    "build": "electron-builder",
    "start": "electron ."
  },
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "build": {
    "appId": "com.yourcompany.sasv",
    "productName": "SASV Workspace",
    "publish": [
      {
        "provider": "github",
        "owner": "Brahmadathanu",
        "repo": "sasv-workspace-dev"
      }
    ],
    "directories": {
      "output": "dist"
    },
    "files": [
      "**/*"
    ],
    "win": {
      "target": [
        "nsis",
        "zip"
      ]
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true
    }
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.50.2",
    "docx": "^8.5.0",
    "electron-prompt": "^1.7.0",
    "electron-updater": "^6.6.2",
    "express": "^5.1.0",
    "flatpickr": "^4.6.13",
    "html-to-docx": "^1.8.0",
    "marked": "^16.2.1",
    "pg": "^8.16.0",
    "sanitize-filename": "^1.6.3",
    "tom-select": "^2.4.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "electron": "^36.2.1",
    "electron-builder": "^26.0.12",
    "electron-reload": "^2.0.0-alpha.1",
    "eslint": "^9.33.0",
    "glob": "^11.0.3",
    "globals": "^16.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Brahmadathanu/sasv-workspace-dev.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/Brahmadathanu/sasv-workspace-dev/issues"
  },
  "homepage": "https://github.com/Brahmadathanu/sasv-workspace-dev#readme"
}
